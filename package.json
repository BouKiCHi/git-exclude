{
    "name": "git-exclude",
    "displayName": "Git Exclude",
    "description": "Edit Git exclude File",
    "version": "0.0.1",
    "publisher": "boukichi",
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "git"
    ],
    "activationEvents": [
        "onCommand:extension.editGitExclude",
        "onCommand:extension.appendGitExclude",
        "onCommand:extension.appendGitExcludeUri"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.editGitExclude",
                "title": "GitExclude: Edit exclude File"
            },
            {
                "command": "extension.appendGitExclude",
                "title": "GitExclude: Append Current File to exclude File"
            },
            {
                "command": "extension.appendGitExcludeUri",
                "title": "GitExclude: Append Current File/Folder to exclude File"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "extension.appendGitExcludeUri",
                    "group": "GitExclude"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
