{
    "name": "git-exclude",
    "displayName": "Git Exclude",
    "description": "Edit Git exclude File",
    "version": "0.3.0",
    "publisher": "boukichi",
    "license": "MIT",
    "icon": "images/icon.png",
    "homepage": "https://github.com/BouKiCHi/git-exclude",
    "repository": {
        "type": "git",
        "url": "https://github.com/BouKiCHi/git-exclude.git"
    },
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "git"
    ],
    "activationEvents": [
        "onCommand:GitExclude.showFileStatus",
        "onCommand:GitExclude.editGitExclude",
        "onCommand:GitExclude.appendGitExcludeUri",
        "onCommand:GitExclude.skipGitWorktreeUri",
        "onCommand:GitExclude.noSkipGitWorktreeUri",
        "onCommand:GitExclude.setAssumeUnchangedUri",
        "onCommand:GitExclude.noAssumeUnchangedUri",
        "onCommand:GitExclude.appendGitExcludeUriSCM",
        "onCommand:GitExclude.skipGitWorktreeUriSCM",
        "onCommand:GitExclude.setAssumeUnchangedSCM"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "GitExclude.showFileStatus",
                "title": "%GitExclude.showFileStatus.title%",
                "category": "GitExclude"
            },
            {
                "command": "GitExclude.editGitExclude",
                "title": "%GitExclude.editGitExclude.title%",
                "category": "GitExclude"
            },
            {
                "command": "GitExclude.appendGitExcludeUri",
                "title": "%GitExclude.appendGitExcludeUri.title%",
                "category": "GitExclude"
            },
            {
                "command": "GitExclude.skipGitWorktreeUri",
                "title": "%GitExclude.skipGitWorktreeUri.title%",
                "category": "GitExclude"
            },
            {
                "command": "GitExclude.noSkipGitWorktreeUri",
                "title": "%GitExclude.noSkipGitWorktreeUri.title%",
                "category": "GitExclude"
            },
            {
                "command": "GitExclude.skipGitWorktreeUri",
                "title": "%GitExclude.skipGitWorktreeUri.title%",
                "category": "GitExclude"
            },
            {
                "command": "GitExclude.noSkipGitWorktreeUri",
                "title": "%GitExclude.noSkipGitWorktreeUri.title%",
                "category": "GitExclude"
            },
            {
                "command": "GitExclude.setAssumeUnchangedUri",
                "title": "%GitExclude.setAssumeUnchangedUri.title%",
                "category": "GitExclude"
            },
            {
                "command": "GitExclude.noAssumeUnchangedUri",
                "title": "%GitExclude.noAssumeUnchangedUri.title%",
                "category": "GitExclude"
            },
            {
                "command": "GitExclude.appendGitExcludeUriSCM",
                "title": "%GitExclude.appendGitExcludeUri.title%",
                "category": "GitExclude"
            },
            {
                "command": "GitExclude.skipGitWorktreeUriSCM",
                "title": "%GitExclude.skipGitWorktreeUri.title%",
                "category": "GitExclude"
            },
            {
                "command": "GitExclude.setAssumeUnchangedSCM",
                "title": "%GitExclude.setAssumeUnchangedUri.title%",
                "category": "GitExclude"
            }
        ],
        "menus": {
            "commandPalette": [
                {
                    "command": "GitExclude.appendGitExcludeUriSCM",
                    "when": "false"
                },
                {
                    "command": "GitExclude.skipGitWorktreeUriSCM",
                    "when": "false"
                },
                {
                    "command": "GitExclude.setAssumeUnchangedSCM",
                    "when": "false"
                }
            ],
            "explorer/context": [
                {
                    "submenu": "git-exclude.explorer-submenu",
                    "group": "GitExclude"
                }
            ],
            "editor/title/context": [
                {
                    "submenu": "git-exclude.explorer-submenu",
                    "group": "GitExclude"
                }
            ],

            "git-exclude.explorer-submenu": [
                {
                    "command": "GitExclude.appendGitExcludeUri",
                    "group": "GitExclude@1"
                },
                {
                    "command": "GitExclude.editGitExclude",
                    "group": "GitExclude@2"
                },
                {
                    "command": "GitExclude.showFileStatus",
                    "group": "GitExclude@3"
                },
                {
                    "submenu": "git-exclude.submenu.skip",
                    "group": "GitExclude@4"
                },
                {
                    "submenu": "git-exclude.submenu.assume",
                    "group": "GitExclude@5"
                }
            ],

            "git-exclude.submenu.skip": [
                {
                    "command": "GitExclude.skipGitWorktreeUri",
                    "group": "GitExcludeSkip@1"
                },
                {
                    "command": "GitExclude.noSkipGitWorktreeUri",
                    "group": "GitExcludeSkip@2"
                }
            ],
            "git-exclude.submenu.assume": [
                {
                    "command": "GitExclude.setAssumeUnchangedUri",
                    "group": "GitExcludeAssume@1"
                },
                {
                    "command": "GitExclude.noAssumeUnchangedUri",
                    "group": "GitExcludeAssume@2"
                }
            ],

            "scm/resourceState/context": [
                {
                    "command": "GitExclude.appendGitExcludeUriSCM",
                    "group": "GitExclude@1"
                },
                {
                    "command": "GitExclude.skipGitWorktreeUriSCM",
                    "group": "GitExclude"
                },
                {
                    "command": "GitExclude.setAssumeUnchangedSCM",
                    "group": "GitExclude"
                }
            ]
        },
        "submenus": [
            {"id": "git-exclude.explorer-submenu", "label": "GitExclude" },
            {"id": "git-exclude.submenu.skip", "label": "Skip" },
            {"id": "git-exclude.submenu.assume", "label": "Assume" }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^8.0.3",
        "@types/node": "^14.6.2",
        "minimist": "^1.2.6",
        "typescript": "^4.0.2",
        "vscode": "^1.1.37"
    },
    "dependencies": {}
}
